<h1>All articles</h1>

<h2 class ="create_button"><%= link_to "Create New Article", new_article_path %></h2>

<% @articles.each do |article| %>

  <h1 class= "title"><td><%= link_to article.title, article %></td></h2>

  <h4 class="delete_button" <td><%= link_to 'Delete', article_path(article), method: :delete, data: { confirm: 'Are you sure?' } %></td></h4>

<% end %>